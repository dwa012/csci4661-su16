<!DOCTYPE html>
<html>
<head>
  <title>Workspace</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>
  <% if current_user %>
    <%= link_to 'Sign out', session_logout_path, method: :delete %>
  <% else  %>
    <%= link_to 'Sign in', session_login_path %>
  <% end %>

<%= yield %>

</body>
</html>
